   "source": [
    "def plusOne(digits):\n",
    "    num = 0\n",
    "    for i in range(len(digits)-1,-1,-1):\n",
    "        num += digits[len(digits)-i-1]*(10**i)\n",
    "    num += 1\n",
    "    return [int(i) for i in str(num)]"
   ]
