   "source": [
    "from collections import Counter\n",
    "def numJewelsInStones(J, S):\n",
    "    ans = 0\n",
    "    dic = Counter(S)\n",
    "    for x in J:\n",
    "        if x in dic:\n",
    "            ans += dic[x]\n",
    "    return ans"
   ]
  },
