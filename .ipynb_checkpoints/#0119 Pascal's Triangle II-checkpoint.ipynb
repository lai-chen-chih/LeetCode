    "def getRow(rowIndex):\n",
    "    ans = []\n",
    "    for j in range(rowIndex+1):\n",
    "        ans.append(int(factorial(rowIndex)/(factorial(j)*factorial(rowIndex-j))))\n",
    "    return ans\n",
    "def factorial(k):\n",
    "    num = 1\n",
    "    for i in range(1,k+1):\n",
    "        num *= i\n",
    "    return num"
   ]
