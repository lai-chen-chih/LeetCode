    "def findTheDifference(s, t):\n",
    "    for x in s:\n",
    "        if x not in t:\n",
    "            return x\n",
    "        else:\n",
    "            t = t[:t.find(x)] + t[t.find(x)+1:]\n",
    "    return t[-1]"
   ]
  },
